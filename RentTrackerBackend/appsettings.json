{
  "ConnectionStrings": {
    "DefaultConnection": "Server=172.168.1.100;Port=5432;Database=renttracker;User Id=renttracker_user;Password=renttracker_user;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Secret": "your-256-bit-secret-key-here-please-change-in-production",
    "Issuer": "rent-tracker-api",
    "Audience": "rent-tracker-client",
    "TokenLifetimeMinutes": 60
  },
  "AllowedHosts": "*"
}
